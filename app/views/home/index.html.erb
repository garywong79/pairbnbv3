<% @list.each do |list|%>
<br><%= list.price %>
<br><%= list.price %>
<br><%= list.description %>
<br><%= list.bathroom %>
<br><%= list.bedroom %>
<br><%= list.bed %>
<br><%= list.property_type %>
<br><%= list.room_type %>


<% list.images.each do |a| %>
	<%= image_tag a.thumb.to_s %>
<% end %>


	<% if logged_in? && current_user.id == list.user.id %>
	<%=link_to "Edit", edit_list_path(list), class: "btn btn-info" %>
	<%= link_to "Delete", list_path(list), method: :delete, class: "btn btn-danger" %>
		<% end %>
		<% if logged_in? && current_user.id != list.user.id %>
			<%= render 'reservations/form', list: list  %>
		<% end %>
	<% end %>



